import React, { Component } from "react";

class Props extends React.Component {
  render() {
    return (
        <Cake src={"https://goo.gl/zo1OdS"} alt={"Cake"} width={"400px"} height={"400px"}/>
        );
  }
};

class Cake extends React.Component {
    render() {
        return (
            <img src={this.props.src} alt={this.props.caption} width={this.props.width} height={this.props.height}  />
            );
    }
};

export default Props;